<dataConfig> 
	<dataSource name="sndg" type="JdbcDataSource" driver="com.mysql.jdbc.Driver" url="jdbc:mysql://192.168.120.136/sndg" user="root" password="1234"/> 

	<document name="persona"> 
		<entity dataSource="sndg" name="item" 
			query="select * from CENTRO"
			pk="id"
			onError="skip"
		>
			<field column="singiid" name="id" />
			<field column="nombre" name="name" />
			<field column="nombre" name="text" />
			
			<entity name="city" query="SELECT 'Buenos Aires'"/>
			<entity name="country_exact" query="SELECT 'Argentina'"/>
			<entity name="type" query="SELECT '30'"/>
	
		</entity>
	</document>
</dataConfig>